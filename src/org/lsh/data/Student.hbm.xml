<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.lsh.data.Student" table="student" schema="" catalog="attendance">
        <id name="studentId">
            <column name="student_id" sql-type="varchar" length="50" not-null="true"/>
        </id>
        <property name="studentName">
            <column name="student_name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="studentPassword">
            <column name="student_password" sql-type="varchar" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>