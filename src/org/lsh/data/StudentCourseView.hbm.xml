<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.lsh.data.StudentCourseView" table="student_course_view" schema="" catalog="attendance">
        <id name="scid">
            <column name="scid" sql-type="int" not-null="true"/>
        </id>
        <property name="studentId">
            <column name="student_id" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="studentName">
            <column name="student_name" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="studentPassword">
            <column name="student_password" sql-type="varchar" length="64" not-null="true"/>
        </property>
        <property name="cid">
            <column name="cid" sql-type="int" not-null="true"/>
        </property>
        <property name="cNo">
            <column name="c_no" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="section">
            <column name="section" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="cTitle">
            <column name="c_title" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="teacher">
            <column name="teacher" sql-type="varchar" length="50" not-null="true"/>
        </property>
        <property name="cTime">
            <column name="c_time" sql-type="varchar" length="45"/>
        </property>
        <property name="capacity">
            <column name="capacity" sql-type="int"/>
        </property>
        <property name="weekdays">
            <column name="weekdays" sql-type="int"/>
        </property>
        <property name="term">
            <column name="term" sql-type="int" not-null="true"/>
        </property>
        <property name="activate">
            <column name="activate" sql-type="tinyint" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>