<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.lsh.data.GradeItem" table="grade_item" schema="" catalog="attendance">
        <id name="itemId">
            <column name="item_id" sql-type="int" not-null="true"/>
        </id>
        <property name="itemName">
            <column name="item_name" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="total">
            <column name="total" sql-type="int" not-null="true"/>
        </property>
        <property name="cid">
            <column name="cid" sql-type="int" not-null="true"/>
        </property>
        <property name="isValid">
            <column name="is_valid" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>