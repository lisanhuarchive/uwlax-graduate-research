<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.lsh.data.StudentCourse" table="student_course" schema="" catalog="attendance">
        <id name="scid">
            <column name="scid" sql-type="int" not-null="true"/>
        </id>
        <property name="studentId">
            <column name="student_id" sql-type="varchar" length="50"/>
        </property>
        <property name="courseId">
            <column name="course_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>