<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.lsh.data.Record" table="record" schema="" catalog="attendance">
        <id name="rid">
            <column name="rid" sql-type="int" not-null="true"/>
        </id>
        <property name="cid">
            <column name="cid" sql-type="int" not-null="true"/>
        </property>
        <property name="isOpen">
            <column name="is_open" sql-type="int" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>